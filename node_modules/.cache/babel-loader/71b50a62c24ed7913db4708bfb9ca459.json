 {\n            await this.$store.dispatch('auth/registerUserWithEmailAndPassword', this.form )\n            this.$router.push('/')\n        }\n    },\n    created () {\n        this.$emit('ready')\n    }\n}\n</script>\n\n<style scoped>\n\n.register {\n    padding: 2em;\n    font-size: 1.5em;\n    align-content: left; /* doing nothing */\n}\n\n.form-group {\n    padding: 0.5em;\n}\n\nlabel {\n    padding: 1em;\n}\n\nbutton {\n    font-size: 0.5em;\n}\n</style>"],"sourceRoot":""}]}