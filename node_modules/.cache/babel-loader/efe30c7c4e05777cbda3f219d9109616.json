div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\nexport default {\n    props: {\n        discussion : {\n            required: true,\n            type: Object\n        }\n    },\n    computed: {\n        ...mapGetters( 'auth', [ 'authUser' ] ),\n    }\n}\n</script>"],"sourceRoot":""}]}